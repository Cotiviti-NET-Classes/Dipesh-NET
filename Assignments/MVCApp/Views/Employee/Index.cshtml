@model List<Employee>;
@{
    ViewData["Title"] = "Employee Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome To Employee Page</h1>
    <p><button>Add Employee</button></p>
    <table>
        <tr>
            <th>Name</th>
            <th>Gender</th>
            <th>Joining Date</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.JoiningDate</td>
            </tr>
        }
    </table>
</div>
